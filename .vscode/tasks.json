{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Start Web",
            "dependsOrder": "sequence",
            "dependsOn": [
                "Restore Web",
                "Web npm start"
            ]
        },
        {
            "label": "Restore Web",
            "type": "shell",
            "command": "azd restore web",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "Web npm start",
            "detail": "Helper task--use 'Start Web' task to ensure environment is set up correctly",
            "type": "shell",
            "command": "npm run dev",
            "options": {
                "cwd": "${workspaceFolder}/src/"
            },
            "presentation": {
                "panel": "dedicated",
            },
            "problemMatcher": []
        }
    ]
}